# PostgreSQL Path
PATH="/Applications/Postgres.app/Contents/MacOS/bin:$PATH"

# PHP Path (Get packages from http://php-osx.liip.ch)
# PATH="/Applications/MAMP/Library/bin:/Applications/MAMP/bin/php/php5.4.4/bin:$PATH"
PATH="/usr/local/php5-20121126-100332/bin:$PATH"

# Mysql Path
PATH="/usr/local/mysql/bin:$PATH"

# Git path
PATH="/usr/local/git/bin:$PATH"

# Go path
PATH="$PATH:/usr/local/go/bin"

# Vagrant path
PATH="$PATH:/opt/vagrant/bin"

# User path
PATH="$PATH:~/.bin"

# Bin path order (brew compained aout this one)
PATH="/usr/local/bin:/usr/bin:$PATH"


export PATH